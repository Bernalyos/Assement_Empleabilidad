{
  "version": 2,
  "name": "nextask-frontend",
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://your-backend-api-url.com/api/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/src/main/resources/static/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/src/main/resources/static/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/main/resources/static/index.html"
    }
  ]
}
